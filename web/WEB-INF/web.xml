<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>UploadHandleServlet</servlet-name>
        <servlet-class>com.servlet.UploadHandleServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>UploadHandleServlet11</servlet-name>
        <servlet-class>com.servlet.UploadHandleServlet11</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>com.servlet.Test</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Identify</servlet-name>
        <servlet-class>com.servlet.IdentifyServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>getScore</servlet-name>
        <servlet-class>com.servlet.getScore</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UploadHandleServlet</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UploadHandleServlet11</servlet-name>
        <url-pattern>/upload1</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Identify</servlet-name>
        <url-pattern>/identify</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>getScore</servlet-name>
        <url-pattern>/score</url-pattern>
    </servlet-mapping>

    <error-page>
        <error-code>404</error-code>
        <location>/Pages/404.html</location>
    </error-page>

</web-app>